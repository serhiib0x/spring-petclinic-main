pipeline {
    agent {label 'agent1'}
    
    tools {
        // Install the Maven
        maven "3.8.6"
    }
    
    stages {
        stage('Pull from GitHub') {
            steps {
                // Get code from a GitHub repository
                git branch: 'main',
                credentialsId: 'serhiib0x',
